CREATE TABLE DataCoSupplyChainDataset(
    data_type                      VARCHAR(10) NOT NULL,
    days_for_shipping_real         SMALLINT,
    days_for_shipment_scheduled    SMALLINT,
    benefit_per_order              FLOAT8,
    sales_per_customer             FLOAT8,
    delivery_status                VARCHAR(20),
    late_delivery_risk             BOOL,
    category_id                    SMALLINT,
    category_name                  VARCHAR(20),
    customer_city                  VARCHAR(20),
    customer_country               VARCHAR(15),
    customer_email                 VARCHAR(10),
    customer_fname                 VARCHAR(15),
    customer_id                    INT PRIMARY KEY NOT NULL,
    customer_lname                 VARCHAR(15),
    customer_password              VARCHAR(10),
    customer_segment               VARCHAR(15),
    customer_state                 VARCHAR(5),
    customer_street                VARCHAR(40),
    customer_zipcode               SMALLINT,
    department_id                  SMALLINT,
    department_name                VARCHAR(20),
    latitude                       SMALLINT,
    longitude                      SMALLINT,
    market                         VARCHAR(15),
    order_city                     VARCHAR(35),
    order_country                  VARCHAR(35),
    order_customer_id              SMALLINT,
    order_date_date_orders         VARCHAR(20),
    order_id                       INT,
    order_item_cardprod_id         INT,
    order_item_discount            SMALLINT,
    order_itemd_discount_rate      SMALLINT,
    order_item_id                  INT,
    order_item_product_price       SMALLINT,
    order_item_profit_ratio        SMALLINT,
    order_item_quantity            INT,
    sales                          SMALLINT,
    order_item_total               SMALLINT,
    order_profit_per_order         SMALLINT,
    order_region                   VARCHAR(15),
    order_state                    VARCHAR(40),
    order_status                   VARCHAR(15),
    order_zipcode                  INT,
    product_card_id                INT,
    product_category_id            SMALLINT,
    product_description            SMALLINT,
    product_image                  VARCHAR(100),
    product_name                   VARCHAR(50),
    product_price                  SMALLINT,
    product_status                 INT,
    shipping_date_date_orders      VARCHAR(20),
    shipping_mode                  VARCHAR(15)
);
